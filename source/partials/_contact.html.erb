
<div id="contact">

  <div class="contact">
    <h2><%= data.contact.head %></h2> 
    <br>
  </div>	

  <div class="container">	
  
    <div class="row">

      <div class="col-md-6">
	<iframe width="100%" height="400px" frameborder="0" scrolling="no" marginheight="0" marginwidth="0"
		src="<%= data.contact.gmap %>">
	</iframe>
      </div>

      <div class="col-md-6">
	<div class="contact-img"><img class="contact-image" src="<%= data.contact.image %>" /></div>
	<div class="contact-details-title"><%= data.contact.name %></div>
	<div class="contact-details"><%= data.contact.address %></div>
	<div class="contact-details"><%= data.contact.email %></div>
      </div>

    </div>

  </div>

</div>

